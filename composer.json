{
    "name": "jetimob/zapsign-sdk-php-laravel",
    "keywords": ["laravel", "zapsign", "signature", "sdk", "digital-signature", "assinatura digital", "assinatura eletronica"],
    "description": "SDK de integração com a ZapSign",
    "type": "library",
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "jetimob/http-php-laravel": "^1.4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^v6.18.0",
        "phpunit/phpunit": "^v9.5.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jetimob\\ZapSign\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jetimob\\ZapSign\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jetimob\\ZapSign\\ZapSignServiceProvider"
            ],
            "aliases": {
                "ZapSign": "Jetimob\\ZapSign\\Facades\\ZapSign"
            }
        }
    },
    "authors": [
        {
            "name": "Jetimob",
            "email": "contato@jetimob.com",
            "homepage": "https://jetimob.com"
        }
    ],
    "support": {
        "issues": "https://github.com/jetimob/zapsign-sdk-php-laravel/issues"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
